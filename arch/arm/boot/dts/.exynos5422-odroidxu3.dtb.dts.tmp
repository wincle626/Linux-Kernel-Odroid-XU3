        "sectbl", "mfc_fw", "mfc_nfw", "secdma";
  contig = <0 0x6650000 0x0>, <2 0x400000 0x0>, <10 0x800000 0x0>,
    <11 0x6000000 0x0>, <7 0x1000000 0x0>, <9 0x400000 0x0>,
    <8 0x400000 0x0>, <12 0x400000 0x0>, <13 0x400000 0x0>;

  ion_noncontig_heap {
   id-type = <0 0>;
  };

  exynos_contig_heap {
